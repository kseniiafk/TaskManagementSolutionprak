{
    "ReverseProxy": {
        "Routes": {
            "users-route": {
                "ClusterId": "users-cluster",
                "Match": {
                    "Path": "/users/{**catch-all}"
                }
            },
            "projects-route": {
                "ClusterId": "projects-cluster",
                "Match": {
                    "Path": "/projects/{**catch-all}"
                }
            },
            "tasks-route": {
                "ClusterId": "tasks-cluster",
                "Match": {
                    "Path": "/tasks/{**catch-all}"
                }
            }
        },
        "Clusters": {
            "users-cluster": {
                "Destinations": {
                    "dest1": {
                        "Address": "http://localhost:5210"
                    }
                }
            },
            "projects-cluster": {
                "Destinations": {
                    "dest1": {
                        "Address": "http://localhost:5065"
                    }
                }
            },
            "tasks-cluster": {
                "Destinations": {
                    "dest1": {
                        "Address": "http://localhost:5124"
                    }
                }
            }
        }
    }
}
